<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travel360">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".trainShedule"></activity>
        <activity android:name=".vehicleDetails" />
        <activity android:name=".rentVehicles" />
        <activity android:name=".rentform" />
        <activity android:name=".vehicleType" />
        <activity android:name=".travelItems" />
        <activity android:name=".signUp" />
        <activity android:name=".signIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>